#!/bin/python3

import keyboard


# Define a function to stimulate the key combinations
def stimulate_key_combinations():
    # Simulate Ctrl+L
    keyboard.press_and_release("ctrl+l")
    # Simulate Ctrl+C
    keyboard.press_and_release("ctrl+c")
    # Simulate Escape
    keyboard.press_and_release("esc")


# Call the function to execute the key combinations
stimulate_key_combinations()
